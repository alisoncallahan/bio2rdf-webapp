bio2rdf_ns:drugbank_drugs a queryall_ns:Namespace , queryall_ns:ValidatingNamespace , queryall_ns:RegexValidatingNamespace ;
    queryall_ns:authority <http://bio2rdf.org/> ;
    queryall_ns:uriTemplate "${authority}${namespace}${separator}${identifier}" ;
    queryall_ns:separator ":" ;
	queryall_ns:preferredPrefix "drugbank_drugs" ;
	queryall_ns:convertToPreferred "true"^^xsd:boolean ;
	queryall_ns:alternativePrefix "" ;
	queryall_ns:validationPossible "false"^^xsd:boolean ;
# FIXME: Confirm this identifier regex works as desired
	queryall_ns:identifierRegex "DB[\\d]+" ;
	dc:title "The FU Berlin Drugbank namespace for drug identifiers." .

