let $server_address := "${server_address}"       
let $result_uri := fn:replace($bio2rdf_uri,$match_regex,$replace_regex)      return 		
<rdf:Description rdf:about="{$result_uri}">
<bio2rdf:hasStandardURI rdf:resource="{$bio2rdf_uri}"/><bio2rdf:transformedWithMatchRegex>{$match_regex}</bio2rdf:transformedWithMatchRegex>                        
<bio2rdf:transformedWithReplaceRegex>{$replace_regex}</bio2rdf:transformedWithReplaceRegex>
<bio2rdf:transformedWithQueryType>{$query_type}</bio2rdf:transformedWithQueryType>
<bio2rdf:transformedWithServerAddress>{$server_address}</bio2rdf:transformedWithServerAddress>
</rdf:Description> 
}         
</rdf:RDF>]]]]><![CDATA[></query></xquery></code></pipe>]]></syntax>
