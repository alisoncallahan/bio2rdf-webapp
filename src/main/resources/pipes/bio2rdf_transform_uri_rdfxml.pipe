<pipeConfig>
  <id>bio2rdf_transform_uri_rdfxml</id>
  <name></name>
  <syntax><![CDATA[<?xml version="1.0" encoding="UTF-8"?><pipe><parameters><parameter><id>bio2rdf_uri</id><label>Bio2RDF URI</label><default>http://bio2rdf.org/ecocyc:REG0-6336</default></parameter><parameter><id>query_type</id><label>Query</label><default>/</default></parameter><parameter><id>server_address</id><label>Bio2RDF Server</label><default>http://localhost</default></parameter><parameter><id>match_regex</id><label>Transform Match Regex</label><default>^http://[^/]*/(.*):(.*)$</default></parameter></parameters><code><xquery><source><simplemix><source><rdffetch format="RDF/XML"><location>noop://localhost/?bio2rdf_uri=%24%7Bbio2rdf_uri%7D&amp;query_type=%24%7Bquery_type%7D&amp;server_address=%24%7Bserver_address%7D&amp;match_regex=%24%7Bmatch_regex%7D</location></rdffetch></source></simplemix></source><contentType>text/xml</contentType><query><![CDATA[xquery version "1.0";
<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:bio2rdf="http://bio2rdf.org/ns/bio2rdf:">
{       
let $bio2rdf_uri := "${bio2rdf_uri}"       
let $match_regex := "${match_regex}" 
let $replace_regex := "${server_address}${query_type}$1:$2"
