<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	
	<display-name>Bio2RDF %%__VERSION__%%</display-name>
	<description>
		Bio2RDF %%__VERSION__%%
	</description>

    <context-param>
        <param-name>VERSION</param-name>
        <param-value>%%__VERSION__%%</param-value>
    </context-param>

    <context-param>
        <param-name>BASE_CONFIG_FILES</param-name>
        <param-value>%%__BASE_CONFIG_FILES__%%</param-value>
    </context-param>
    <context-param>
        <param-name>BASE_CONFIG_URI</param-name>
        <param-value>http://bio2rdf.org/webapp_configuration:defaultBaseConfig</param-value>
    </context-param>
    <context-param>
        <param-name>BASE_CONFIG_MIME_FORMAT</param-name>
        <param-value>text/rdf+n3</param-value>
    </context-param>
    <context-param>
        <param-name>CONFIG_API_VERSION</param-name>
        <param-value>3</param-value>
    </context-param>
	<context-param>
	    <param-name>SUBVERSION_INFO</param-name>
        <param-value>$Id: web-production-template.xml 975 2011-02-23 00:59:00Z p_ansell $</param-value>
	</context-param>
	
	<welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

	<filter> 
		 <filter-name>UrlRewriteFilter</filter-name> 
		 <filter-class>org.tuckey.web.filters.urlrewrite.UrlRewriteFilter</filter-class> 
		
         <init-param> 
             <param-name>logLevel</param-name> 
             <param-value>log4j</param-value> 
         </init-param> 
    </filter> 
    
	<filter-mapping> 
		<filter-name>UrlRewriteFilter</filter-name> 
		<url-pattern>/*</url-pattern> 
        <dispatcher>REQUEST</dispatcher> 
        <dispatcher>FORWARD</dispatcher> 
	</filter-mapping>
	
    <mime-mapping>
		<extension>owl</extension>
		<mime-type>application/rdf+xml</mime-type>
	</mime-mapping>
	
	<mime-mapping>
		<extension>rdf</extension>
		<mime-type>application/rdf+xml</mime-type>
	</mime-mapping>

	<mime-mapping>
		<extension>n3</extension>
		<mime-type>text/rdf+n3</mime-type>
	</mime-mapping>
</web-app>
